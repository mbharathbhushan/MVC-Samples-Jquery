@model  _08___DataAccessInMVCUsingEntityFramework.Models.Employee
@{
    ViewBag.Title = "Employee";
}

<h2>Employee Details</h2>
<table>
    <tr>
        <td>Employee ID:</td>
        <td>@Model.EmployeeId</td>
    </tr>
    <tr>
        <td>Employee Name:</td>
        <td>@Model.Name </td>
    </tr>
    <tr>
        <td>Employee Gender:</td>
        <td>@Model.Gender</td>
    </tr>
    <tr>
        <td>City:</td>
        <td>@Model.City</td>
    </tr>
</table>

